	status = "okay";
};

&usbdrd_dwc3_1 {
	status = "okay";
	dr_mode = "host";
};

&usb_host0_ehci {
	status = "okay";
};

&usb_host0_ohci {
	status = "okay";
};

&usb_host1_ehci {
	status = "okay";
};

&usb_host1_ohci {
	status = "okay";
};

&uart0 {
	pinctrl-names = "default";
	pinctrl-0 = <&uart0_xfer &uart0_cts>;
	status = "okay";
};

&fiq_debugger {
	rockchip,baudrate = <115200>;  /* Only 115200 and 1500000 */
	status = "okay";
};

&uart2 {
	status = "disabled";
};

&uart4 {
	status = "okay";
};

&vpu {
	status = "okay";
};

&threshold {
	temperature = <85000>;
};

&target {
	temperature = <100000>;
};

&soc_crit {
	temperature = <105000>;
};

&tsadc {
	/* tshut mode 0:CRU 1:GPIO */
	rockchip,hw-tshut-mode = <1>;
	/* tshut polarity 0:LOW 1:HIGH */
	rockchip,hw-tshut-polarity = <1>;
	rockchip,hw-tshut-temp = <110000>;
	status = "okay";
};

&pinctrl {
	pinctrl-names = "default";
	pinctrl-0 = <&usb_hub_rst>, <&usb1v2_en>;
	pmic {
		pmic_int_l: pmic-int-l {
			rockchip,pins =
				<1 21 RK_FUNC_GPIO &pcfg_pull_up>;
		};

		pmic_dvs2: pmic-dvs2 {
			rockchip,pins =
				<1 18 RK_FUNC_GPIO &pcfg_pull_down>;
		};

		vsel1_gpio: vsel1-gpio {
			rockchip,pins =
				<1 17 RK_FUNC_GPIO &pcfg_pull_down>;
		};

		vsel2_gpio: vsel2-gpio {
			rockchip,pins =
			<1 14 RK_FUNC_GPIO &pcfg_pull_down>;
		};
	};

	sdmmc {
		sdio_pwr_en: sdio-pwr-en {
			rockchip,pins =
				<0 9 RK_FUNC_GPIO &pcfg_pull_up>; //GPIO0_B1 - PMUIO2_VOLSEL
		};
	};

	sata {
		sata_drv: sata-drv {
			rockchip,pins =
				<1 0 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	rgmii_phys {
		rgmii_phy_reset: rgmii-phy-reset {
			rockchip,pins = <3 16 RK_FUNC_GPIO &pcfg_pull_down>;
		};
	};

	usb {
		usb4_en: usb4-en {
			rockchip,pins =
				<4 23 RK_FUNC_GPIO &pcfg_pull_up>; // GPIO4_C7 / USB4_EN_OC_1V8
		};
		usb5_en: usb5-en {
			rockchip,pins =
				<4 24 RK_FUNC_GPIO &pcfg_pull_up>; // GPIO4_D0 / USB5_EN_OC_1V8
		};
		otg_vbus_drv: otg-vbus-drv {
			rockchip,pins =
				<2 1 RK_FUNC_GPIO &pcfg_pull_none>; // GPIO2_A1 / USB0_EN_OC_1V8#
		};
	};

	usb_hub {
		usb1v2_en: usb1v2-en {
			rockchip,pins =
				<1 4 RK_FUNC_GPIO &pcfg_pull_up>; // GPIO1_A4 / VCC_1V2_EN
		};
		usb_hub_rst: usb-hub-rst {
			rockchip,pins =
				<4 26 RK_FUNC_GPIO &pcfg_pull_up>; // GPIO4_D2 / USBHUB_RST_1V8
		};
	};

	vdp0 {
		dp_det: dp-det {
			rockchip,pins = <4 25 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	wireless-wlan {
		compatible = "wlan-platdata";
		rockchip,grf = <&grf>;
		wifi_chip_type = "rtl8723bs";
		sdio_vref = <1800>;
		WIFI,host_wake_irq = <&gpio0 3 GPIO_ACTIVE_HIGH>;
		status = "okay";
	};

     pcie {
        pcie_drv: pcie-drv {
                        rockchip,pins =
                                <4 21 RK_FUNC_GPIO &pcfg_pull_none>;
                        };

        pinctrl_pcie1: pinctrl-pcie1 {
                      rockchip,pins =
                                <2 28 RK_FUNC_GPIO &pcfg_pull_up>;
                };
        };

		wireless-bluetooth {
			uart0_gpios: uart0-gpios {
				rockchip,pins =
					<2 19 RK_FUNC_GPIO &pcfg_pull_none>;
			};
	};
};
