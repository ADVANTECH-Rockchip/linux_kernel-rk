			rockchip,pins = <4 24 RK_FUNC_1 &pcfg_pull_up_12ma>;
		};

		sdio0_clk: sdio0-clk {
			rockchip,pins = <4 25 RK_FUNC_1 &pcfg_pull_none_12ma>;
		};

