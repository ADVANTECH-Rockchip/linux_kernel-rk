			rockchip,pins = <4 20 RK_FUNC_1 &pcfg_pull_up_12ma>;
		};

		sdio0_bus4: sdio0-bus4 {
			rockchip,pins = <4 20 RK_FUNC_1 &pcfg_pull_up_12ma>,
